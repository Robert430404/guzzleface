{
  "name": "robert430404/guzzleface",
  "description": "This is a library that helps you write guzzle clients using an OpenFeign like interface.",
  "type": "library",
  "require": {
    "guzzlehttp/guzzle": "^6.3",
    "doctrine/annotations": "^1.6",
    "memio/memio": "^2.0",
    "league/flysystem": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.20",
    "mockery/mockery": "1.4.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Joseph Cox Jr",
      "email": "robert@studiorclv.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Robert430404\\GuzzleFace\\": "src/GuzzleFace"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Robert430404\\GuzzleFace\\Tests\\": "tests/GuzzleFace"
    }
  }
}
